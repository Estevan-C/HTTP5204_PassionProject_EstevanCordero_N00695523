@model PersonalVideoGameLibrary.Models.ConsoleDto 

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you wish to delete this Console</h2>
<a class="btn btn-default" href="/Console/List">Cancel</a>
<h3>Console Name: @Model.ConsoleName</h3>
<form action="/Console/Delete/@Model.ConsoleID" method="POST">
    <input type="submit" value="Confirm Delete" />
</form>

